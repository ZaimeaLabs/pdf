{
    "name": "zaimealabs/pdf",
    "license": "MIT",
    "homepage": "https://zaimea.com",
    "description": "The ZaimeaLabs PDF package.",
    "keywords": [
        "invoices",
        "laravel",
        "pdf",
        "reports",
        "zaimealabs"
    ],
    "authors": [
        {
            "name": "Custură Laurențiu",
            "email": "mail@custura.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-bcmath": "*",
        "dompdf/dompdf": "^2.0",
        "nesbot/carbon": "^2.67|^3.0",
        "illuminate/view": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "ZaimeaLabs\\PDF\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZaimeaLabs\\PDF\\PDFServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
